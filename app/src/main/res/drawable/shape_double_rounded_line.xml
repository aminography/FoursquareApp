<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">

            <size
                android:width="30dp"
                android:height="7dp" />

        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:gravity="top"
        android:top="0dp">

        <shape android:shape="rectangle" >

            <solid android:color="#5F6268" />

            <!--<stroke android:color="#5F6268" android:width="1dp"/>-->

            <corners android:radius="1dp" />

            <size
                android:width="30dp"
                android:height="2dp" />

        </shape>

    </item>

    <item
        android:bottom="7dp"
        android:gravity="bottom"
        android:top="5dp">

        <shape android:shape="rectangle">

            <solid android:color="#5F6268" />

            <corners android:radius="1dp" />

            <size
                android:width="30dp"
                android:height="2dp" />

        </shape>

    </item>

</layer-list>